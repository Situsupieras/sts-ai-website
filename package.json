  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:optimized": "NODE_OPTIONS='--max-old-space-size=512' next build",
    "start": "next start -p 3000",
    "start:optimized": "NODE_OPTIONS='--max-old-space-size=512' next start -p 3000",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true next build",
    "clean": "rm -rf .next out",
    "clean:all": "rm -rf .next out node_modules/.cache",
    "monitor": "node scripts/daily-monitoring.js",
    "deploy:vps": "./scripts/deploy-vps.sh",
    "optimize": "./scripts/optimize-vps.sh",
    "setup:cron": "./scripts/setup-cron.sh",
    "test:monitoring": "./scripts/test-monitoring.sh",
    "monitor:resources": "./scripts/monitor-resources.sh",
    "cleanup": "./scripts/cleanup.sh",
    "maintenance": "./scripts/maintenance.sh"
  },
  "dependencies": {
